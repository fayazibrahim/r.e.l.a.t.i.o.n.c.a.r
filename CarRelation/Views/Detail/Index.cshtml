@model List<Detail>
@{
    ViewData["Title"] = "Index";
}

<h1>Car</h1>

<div class="list-group">
    <ul class="list-group">
        @foreach (Detail detail in Model)
        {
            <li class="list-group-item">
                <a asp-controller="Detail" asp-action="Detail" asp-route-id="@detail.Id">
                    @detail.detail

                </a>
            </li>
        }
    </ul>
</div>