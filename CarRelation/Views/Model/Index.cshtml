@model List<Model>
@{
    ViewData["Title"] = "Index";
}

<h1>Model</h1>

<div class="list-group">
    <ul class="list-group">
        @foreach (Model model1 in Model)
        {
            <li class="list-group-item">
                <a asp-controller="Car" asp-action="Index" asp-route-id="@model1.Id">
                    @model1.model
                    
           
                </a>
            </li>
        }
    </ul>
</div>


